<mapper namespace="com.jisung.mapper.BookMapper">

	<insert id="insert">
		insert into tbl_book(bookid,boardid,thumbnail,title,authors,url) values(seq_book.nextval,#{boardId},#{thumbnail},#{title},#{authors},#{url})
	</insert>
</mapper>